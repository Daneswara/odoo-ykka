<odoo>
	<data>
		<record id="view_logistik_tender_cancel" model="ir.ui.view">
			<field name="name">Pembatalan Tender</field>
			<field name="model">logistik.tender.cancel</field>
			<field name="arch" type="xml">
				<form string="Pembatalan Tender" version="7.0">
					<group>
						<field name="cancel_reason"/>
					</group>
					<footer>
						<button name="tender_cancel" string="SETUJU TENDER INI DIBATALKAN" type="object" class="oe_highlight"/> atau<button string="Batal" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_logistik_tender_cancel" model="ir.actions.act_window">
			<field name="name">Pembatalan Tender</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">logistik.tender.cancel</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="context">{'record_id' : active_id}</field>
			<field name="target">new</field>
		</record>
	</data>
</odoo>