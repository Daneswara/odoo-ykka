<odoo>
	<data>
		<record model="ir.module.category" id="logistik_pengadaan_category">
			<field name="name">LOG4 - Pengadaan</field>
			<field name="description">Pengadaan Material</field>
		</record>
		
		<record model="res.groups" id="group_user">
			<field name="name">User</field>
			<field name="category_id" ref="logistik_pengadaan_category"/>
		</record>

		<record model="res.groups" id="group_logistik">
			<field name="name">Logistik</field>
			<field name="category_id" ref="logistik_pengadaan_category"/>
			<field name="implied_ids" eval="[(4, ref('ka_logistik_pengadaan.group_user'))]"/>
		</record>
		
		<record model="res.groups" id="group_manager">
			<field name="name">Manager</field>
			<field name="category_id" ref="logistik_pengadaan_category"/>
			<field name="implied_ids" eval="[(4, ref('ka_logistik_pengadaan.group_logistik'))]"/>
		</record>
	</data>
</odoo>
