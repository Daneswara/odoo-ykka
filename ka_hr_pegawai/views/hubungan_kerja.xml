<odoo>
	<data>
		<record model="ir.ui.view" id="ka_hr_hubungan_kerja_form">
			<field name="name">Hubungan Kerja Form</field>
			<field name="model">ka_hr.hubungan.kerja</field>
			<field name="arch" type="xml">
				<form string="Hubungan Kerja Karyawan">
					<sheet>
						<group>
							<field name="name"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="ka_hr_hubungan_kerja_tree">
			<field name="name">Hubungan Kerja Tree</field>
			<field name="model">ka_hr.hubungan.kerja</field>
			<field name="arch" type="xml">
				<tree string="Hubungan Kerja Karyawan">
					<field name="name"/>
				</tree>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_ka_hr_hubungan_kerja">
			<field name="name">Hubungan Kerja Karyawan</field>
			<field name="res_model">ka_hr.hubungan.kerja</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Input data hubungan kerja terlebih dahulu.</p>
			</field>
		</record>

		<menuitem action="action_ka_hr_hubungan_kerja" id="menu_sdm_hub_kerja" parent="sub_menu_master" sequence="6" />
	</data>
</odoo>