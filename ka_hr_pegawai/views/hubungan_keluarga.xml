<odoo>
	<data>
		<record model="ir.ui.view" id="ka_hr_hub_keluarga_form">
			<field name="name">ka_hr.hubungan.keluarga.form</field>
			<field name="model">ka_hr.hubungan.keluarga</field>
			<field name="arch" type="xml">
				<form string="Hubungan Keluarga Karyawan">
					<sheet>
						<group>
							<field name="name"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="ka_hr_hub_keluarga_tree">
			<field name="name">ka_hr.hubungan.keluarga.tree</field>
			<field name="model">ka_hr.hubungan.keluarga</field>
			<field name="arch" type="xml">
				<tree string="Hubungan Keluarga Karyawan">
					<field name="name"/>
				</tree>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_ka_hr_hub_keluarga">
			<field name="name">Hubungan Keluarga Karyawan</field>
			<field name="res_model">ka_hr.hubungan.keluarga</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Input data hubungan keluarga terlebih dahulu.</p>
			</field>
		</record>

		<menuitem action="action_ka_hr_hub_keluarga" id="menu_sdm_hub_keluarga" parent="sub_menu_master" sequence="7" groups="hr.group_hr_user"/>
	</data>
</odoo>