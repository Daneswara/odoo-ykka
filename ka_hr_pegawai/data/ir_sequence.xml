<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="hr_employee_sp_sequence_dir" model="ir.sequence">
			<field name="name">HR Employee SP Direksi</field>
			<field name="code">hr.employee.sp.dir</field>
			<field name="prefix">%(range_month)s/DIR/RHS/%(year)s/</field>
			<field name="padding">3</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.kantor_direksi"/>
		</record>

		<record id="hr_employee_sp_sequence_kba" model="ir.sequence">
			<field name="name">HR Employee SP Kebonagung</field>
			<field name="code">hr.employee.sp.kba</field>
			<field name="prefix">%(range_month)s/KBA/RHS/%(year)s/</field>
			<field name="padding">3</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.pg_kebonagung"/>
		</record>

		<record id="hr_employee_sp_sequence_tk" model="ir.sequence">
			<field name="name">HR Employee SP Trangkil</field>
			<field name="code">hr.employee.sp.tk</field>
			<field name="prefix">%(range_month)s/TK/RHS/%(year)s/</field>
			<field name="padding">3</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.pg_trangkil"/>
		</record>
	</data>
</odoo>