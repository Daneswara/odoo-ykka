<odoo>
	<data>
		<record model="ir.actions.act_window" id="action_log_action">
            <field name="name">Fingerspot Action Log</field>
            <field name="res_model">ka_fingerspot.action.log</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Input data fingerspot action log.</p>
            </field>
        </record>

        <record id="view_action_log_form" model="ir.ui.view">
            <field name="name">ka_fingerspot.action.log</field>
            <field name="model">ka_fingerspot.action.log</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <form string="action_log">
                <header>
                    <field name="state" widget="statusbar"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="date_action"/>
                            <field name="command"/>
                            <field name="log_message"/>
                            <field name="user_id"/>
                            <field name="device_id"/>
                        </group>
                    </group>
                </sheet>
                </form>
            </field>
        </record>

        <record id="view_action_log_tree" model="ir.ui.view">
            <field name="name">ka_fingerspot.action.log</field>
            <field name="model">ka_fingerspot.action.log</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="action_log">
                    <field name="date_action"/>
                    <field name="command"/>
                    <field name="log_message"/>
                    <field name="user_id"/>
                    <field name="device_id"/>
                </tree>
            </field>
        </record>
	</data>
</odoo>