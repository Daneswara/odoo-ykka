<odoo>
	<data>
        <record model="ir.ui.view" id="view_scanlog_filter">
            <field name="name">kka_fingerspot.scanlog</field>
            <field name="model">ka_fingerspot.scanlog</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Pencarian">
                    <field name="user_pin_id"/>
                    <field name="pin"/>
                </search>
            </field>
        </record>

		<record model="ir.actions.act_window" id="scanlog_action">
            <field name="name">Fingerspot Scanlog</field>
            <field name="res_model">ka_fingerspot.scanlog</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Input data fingerspot scanlog.</p>
            </field>
        </record>

        <record id="view_scanlog_form" model="ir.ui.view">
            <field name="name">ka_fingerspot.scanlog</field>
            <field name="model">ka_fingerspot.scanlog</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <form string="scanlog">
                <sheet>
                    <group>
                        <group>
                            <field name="device_id"/>
                            <field name="serial_number"/>
                            <field name="scan_date"/>
                            <field name="date_scan_date"/>
                            <field name="pin"/>
                            <field name="verify_mode"/>
                            <field name="io_mode"/>
                            <field name="work_code"/>
                            <field name="user_pin_id"/>
                        </group>
                    </group>
                </sheet>
                </form>
            </field>
        </record>

        <record id="view_scanlog_tree" model="ir.ui.view">
            <field name="name">ka_fingerspot.scanlog</field>
            <field name="model">ka_fingerspot.scanlog</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="scanlog">
                    <field name="device_id"/>
                    <field name="serial_number"/>
                    <field name="scan_date"/>
                    <field name="date_scan_date"/>
                    <field name="pin"/>
                    <field name="verify_mode"/>
                    <field name="io_mode"/>
                    <field name="work_code"/>
                    <field name="user_pin_id"/>
                </tree>
            </field>
        </record>
	</data>
</odoo>