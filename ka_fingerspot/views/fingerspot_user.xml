<odoo>
	<data>
          <record model="ir.ui.view" id="view_user_filter">
            <field name="name">ka_fingerspot.user</field>
            <field name="model">ka_fingerspot.user</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Pencarian">
                    <field name="name"/>
                    <field name="pin"/>
                </search>
            </field>
        </record>

		<record model="ir.actions.act_window" id="user_action">
            <field name="name">Fingerspot User</field>
            <field name="res_model">ka_fingerspot.user</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Input data user fingerspot.</p>
            </field>
        </record>

        <record id="view_user_form" model="ir.ui.view">
            <field name="name">ka_fingerspot.user</field>
            <field name="model">ka_fingerspot.user</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <form string="user">
                <sheet>
                    <group>
                        <group>
                            <field name="pin"/>
                            <field name="name"/>
                            <field name="password"/>
                            <field name="rfid"/>
                            <field name="privilege"/>
                        </group>
                    </group>
                    <notebook colspan="4">
                        <page string="Data Scan" autofocus="autofocus">
                            <field name="template_lines">
                                <tree>
                                    <field name="user_pin_id"/>
                                    <field name="index"/>
                                    <field name="alg_version"/>
                                    <field name="template"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
                </form>
            </field>
        </record>

        <record id="view_user_tree" model="ir.ui.view">
            <field name="name">ka_fingerspot.user</field>
            <field name="model">ka_fingerspot.user</field>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <tree string="user">
                    <field name="pin"/>
                    <field name="name"/>
                    <field name="password"/>
                    <field name="rfid"/>
                    <field name="privilege"/>
                </tree>
            </field>
        </record>
	</data>
</odoo>