<odoo>
	<data>
		<record model="ir.ui.view" id="kab_kota_search_view">
            <field name="name">res.kab.kota.search</field>
            <field name="model">res.kab.kota</field>
            <field name="arch" type="xml">
                <search>
                    <field name="code"/>
					<field name="name"/>
                </search>
            </field>
        </record>

		<record model="ir.ui.view" id="view_form_kab_kota">
			<field name="name">Form Data Kab/Kota</field>
			<field name="model">res.kab.kota</field>
			<field name="arch" type="xml">
				<form string="Form Data Kabupaten">
					<sheet>
						<group>
							<field name="code"/>
							<field name="name"/>
							<field name="provinsi_id"/>
							<field name="company_id" groups="base.group_multi_company" string="Unit/PG"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="view_tree_kab_kota">
			<field name="name">Data Kab/Kota</field>
			<field name="model">res.kab.kota</field>
			<field name="arch" type="xml">
				<tree>
					<field name="code"/>
					<field name="name"/>
					<field name="provinsi_id"/>
					<field name="company_id" groups="base.group_multi_company" string="Unit/PG"/>
				</tree>
			</field>
		</record>
		<record model="ir.actions.act_window" id="action_kabupaten">
			<field name="name">Data kab.kota</field>
			<field name="res_model">res.kab.kota</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Data Kab/Kota masih kosong. Input terlebih dahulu.</p>
			</field>
		</record>
	</data>
</odoo>