<odoo>
	<data>
		<record model="ir.ui.view" id="gps_uploader_wizard_form_view">
            <field name="name">gps.uploader.form</field>
            <field name="model">gps.uploader.wizard</field>
            <field name="arch" type="xml">
                <form string="GPS Uploader Form">
	                <group>
	                    <field name="file_gps"/>
	                </group>
	                <footer>
	                 	<button name="proses_gps_to_text" type="object" string="Save"/> or 
	                 	<button name="" string="Cancel" special="cancel" class="oe_link"/> 
	                </footer>
	            </form>
            </field>
        </record>

        <record id="action_view_gps_upload" model="ir.actions.act_window">
            <field name="name">Upload Data</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">gps.uploader.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="gps_uploader_wizard_form_view"/>
            <field name="target">new</field>
        </record>
	</data>
</odoo>