<odoo>
    <data>

      <record model="ir.ui.view" id="view_form_zona_rehabilitasi">
      <field name="name">Form Zona Rehabilitasi</field>
      <field name="model">ka_plantation.zona.rate</field>
      <field name="arch" type="xml">
        <form string="Form Zona Rehabilitasi">
          <sheet>
            <group>
              <field name="name"/>
              <field name="tarif"/>
              <field name="company_id" groups="base.group_multi_company"/>
            </group>
            <notebook colspan="4">
              <page String="List Desa">
                <field name="desa_ids"/>
              </page>
            </notebook>
          </sheet>
        </form>
      </field>
    </record>

    <record model="ir.ui.view" id="view_tree_zona_rehabilitasi">
      <field name="name">Data Zona Rehabilitasi</field>
      <field name="model">ka_plantation.zona.rate</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="tarif"/>
          <field name="company_id" groups="base.group_multi_company"/>
        </tree>
      </field>
    </record>

        <record model="ir.actions.act_window" id="action_zona_rehabilitasi">
            <field name="name">Zona Rehabilitasi</field>
            <field name="res_model">ka_plantation.zona.rate</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Data Zona Rehabilitasii masih kosong. Input terlebih dahulu.
                </p>
            </field>
        </record>
    </data>
</odoo>