<odoo>
	<data>
		<record model="ir.ui.view" id="view_form_periode">
			<field name="name">Form Periode Tanam Tebang</field>
			<field name="model">ka_plantation.periode</field>
			<field name="arch" type="xml">
				<form string="Form Periode Tanam Tebang">
					<sheet>
						<group>
							<field name="name"/>
							<field name="keterangan"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="view_tree_periode">
			<field name="name">Data Periode Tanam Tebang</field>
			<field name="model">ka_plantation.periode</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="keterangan"/>
				</tree>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_periode">
			<field name="name">Periode Tanam Tebang</field>
			<field name="res_model">ka_plantation.periode</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Data periode tanam tebang masih kosong. Input terlebih dahulu.</p>
			</field>
		</record>
	</data>
</odoo>