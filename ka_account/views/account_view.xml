<odoo>
    <data>
		<record id="account.menu_action_account_moves_all" model="ir.ui.menu">
			<field name="name">Journal Items</field>
			<field name="groups_id" eval="[(6,0,[])]"/>
		</record>
    	
        <record id="view_account_withparent_form" model="ir.ui.view">
        <field name="name">ka_account.account.form</field>
        <field name="model">account.account</field>
        <field name="inherit_id" ref="account.view_account_form"/>
        <field name="arch" type="xml">
            <field name="deprecated" position="after">
                <field name="parent_id" domain="[('internal_type','=','view')]" groups="ka_account.group_coas_user" 
                    context="{'show_parent_account':True}" />
                    <field name="account_template_id" required="1"/>
                    <field name="note"/> 
                    <notebook colspan="2">
                        <page string="Group Report">
                            <field name="report_line_ids" nolabel="1">
                                <tree>
                                    <field name="code"/>
                                    <field name="name"/>
                                    <field name="type"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
            </field>
        </field>
        </record>

        <record id="account.action_account_form" model="ir.actions.act_window">
        <field name="context">{'show_parent_account':True}</field>
        </record>
        <menuitem action="account.action_account_type_form" id="menu_action_account_type_form" 
            parent="account.account_account_menu" sequence="1" groups="ka_account.group_coas_user"/>
            

           <record id="view_account_tree" model="ir.ui.view">
            <field name="name">ka_account.account.tree</field>
            <field name="model">account.account</field>
            <field name="field_parent">child_ids</field>
            <field name="arch" type="xml">
                <tree string="Chart of accounts" toolbar="1" colors="blue:internal_type=='view'">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="parent_id" invisible="1"/>
                    <field name="user_type_id"/>
                    <field name="internal_type" invisible="1"/>
                    <field name="currency_id" options="{'no_create': True}" groups="base.group_multi_currency"/>
                    <field name="company_id" options="{'no_create': True}" groups="base.group_multi_company"/>
                    <field name="debit" sum="Total Debit"/>
                    <field name="credit" sum="Total Credit"/>
                    <field name="balance" sum="Total Balance"/>
                </tree>
            </field>
        </record>

        <record id="open_view_account_tree" model="ir.actions.act_window">
        <field name="name">CoA Heirarchy</field>
        <field name="res_model">account.account</field>
        <field name="view_type">tree</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_account_tree"/>
        <field name="domain">[('parent_id','=',False)]</field>
        <field name="context">{'show_parent_account':True}</field>
        </record>
        <record id="view_account_noparent_tree" model="ir.ui.view">
            <field name="name">ka_account.account.tree</field>
            <field name="model">account.account</field>
            <field name="arch" type="xml">
                <tree string="Chart of accounts" colors="blue:internal_type=='view'">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="parent_id" invisible="1" />
                    <field name="user_type_id"/>
                    <field name="internal_type" invisible="1"/>
                    <field name="currency_id" options="{'no_create': True}" groups="base.group_multi_currency"/>
                    <field name="company_id" options="{'no_create': True}" groups="base.group_multi_company"/>
                    <field name="debit" sum="Total Debit"/>
                    <field name="credit" sum="Total Credit"/>
                    <field name="balance" sum="Total Balance"/>
                </tree>
            </field>
        </record>



        <record id="open_view_account_noparent_tree" model="ir.actions.act_window">
        <field name="name">CoA Heirarchy</field>
        <field name="res_model">account.account</field>
        <field name="view_type">tree</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_account_noparent_tree"/>
        <field name="context">{'show_parent_account':True}</field>
        </record>

        <record id="action_move_lines_open" model="ir.actions.act_window">
            <field name="name">Journal Entry Items</field>
            <field name="res_model">account.move.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('account_id','child_of',[active_id])]</field>
        </record>

        <record id="action_open_account" model="ir.values">
            <field eval="'tree_but_open'" name="key2"/>
            <field eval="'account.account'" name="model"/>
            <field name="name">Accounts</field>
            <field eval="'ir.actions.act_window,%d'%action_move_lines_open" name="value"/>
        </record>

        <record id="view_ka_account_payable_category_tree" model="ir.ui.view">
            <field name="name">ka.account.payable.category.tree</field>
            <field name="model">ka_account.payable.category</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="property_account_payable_id"/>
                </tree>
            </field>
        </record>
        
        <record id="view_ka_account_payable_category_form" model="ir.ui.view">
            <field name="name">ka.account.payable.category.form</field>
            <field name="model">ka_account.payable.category</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="2">
                            <field name="code"/>
                            <field name="name"/>
                            <field name="property_account_payable_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="action_open_ka_account_payable_category" model="ir.actions.act_window">
            <field name="name">Kategori Pembelian</field>
            <field name="res_model">ka_account.payable.category</field>
            <field name="view_type">form</field>                      
        </record>
        <menuitem id="menu_ka_account_payable_category" action="action_open_ka_account_payable_category" parent="account.account_account_menu" sequence="7"/> 
    </data>
</odoo>