<odoo>
	<data> 
		<record id="view_bank_statement_line_import_wizard" model="ir.ui.view">
            <field name="name">ka.bank.statement.line.import</field>
            <field name="model">bank.statement.line.import.wizard</field>
            <field name="arch" type="xml">
                 <form string="Import Transaction">
                    <div class="col-xs-6">
                        <group>
                            <field name="data_file" required="1"/>
                        </group>
                    </div>
                    <footer>
                        <button name="import_bank_statement_line" string="Import" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
	</data>
</odoo>


