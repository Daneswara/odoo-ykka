<odoo>
	<data noupdate="1">
		<record model="ir.cron" id="ir_cron_weekly_report_mail">
			<field name="name">Periodic Weekly Report Mail</field>
			<field eval="True" name="active"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="priority">1</field>
			<field name="interval_number">1</field>
			<field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 12:30:00')"/>
			<field name="interval_type">days</field>
			<field name="numbercall">-1</field>
			<field eval="'ka_hr_attendance.report.weekly'" name="model"/>
			<field eval="'cron_weekly_report_mail'" name="function"/>
			<field eval="'()'" name="args"/>
		</record>
	</data>
</odoo>