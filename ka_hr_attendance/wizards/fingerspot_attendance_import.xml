<odoo>
	<data>
		<record model="ir.ui.view" id="wizard_ka_hr_fingerspot_attendance_import_form">
			<field name="name">Import Presensi Pegawai</field>
			<field name="model">ka_hr.fingerspot.attendance.import.wizard</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import Presensi Pegawai">
					<group>
						<field name="date_from"/>
						<field name="date_to"/>
						<field name="company_id" groups="base.group_multi_company"/>
					</group>
					<footer>
						<button name="import_data" string="Import" type="object" class="btn-primary" attrs="{'invisible': ['|', ('date_from', '=', False), ('date_to', '=', False)]}"/>
						<button string="Batal" class="btn-default" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>

		<act_window
			id="action_wizard_ka_hr_fingerspot_attendance_import"
			name="Import Presensi Pegawai"
			res_model="ka_hr.fingerspot.attendance.import.wizard"
			view_mode="form"
			target="new"/>

		<menuitem action="action_wizard_ka_hr_fingerspot_attendance_import" id="menu_wizard_ka_hr_fingerspot_attendance_import" parent="hr_attendance.menu_hr_attendance_manage_attendances" sequence="99" groups="hr_attendance.group_hr_attendance_user"/>
	</data>
</odoo>