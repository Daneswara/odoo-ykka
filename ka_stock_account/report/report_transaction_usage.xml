<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->

        <report
	        id="report_transaction_usage"
	        model="stock.picking"
	        string="Transaksi Pemakaian"
	        name="ka_stock_account.template_report_transaction_usage"
	        file="ka_stock_account.template_report_transaction_usage" 
	        report_type="qweb-pdf" 
	        menu="True" 
		/>        
        
         <record id="paperformat_transaction_usage" model="report.paperformat">
            <field name="name">Transaksi Pemakaian</field>
            <field name="default" eval="True" />
            <field name="format">custom</field>
            <field name="page_height">280</field>
            <field name="page_width">215</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">30</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">28</field>
            <field name="dpi">90</field>
        </record>
		<record id="ka_stock_account.report_transaction_usage" model="ir.actions.report.xml">
		    <field name="paperformat_id" ref="ka_stock_account.paperformat_transaction_usage" />
		</record>	
	</data>
</odoo>

