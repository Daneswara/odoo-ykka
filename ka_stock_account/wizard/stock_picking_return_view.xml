<odoo>
	<data>
		<record id="view_stock_return_note_picking_form" model="ir.ui.view">
	        <field name="name">Return lines</field>
	        <field name="model">stock.return.picking</field>
	        <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
	        <field name="arch" type="xml">
	        	<xpath expr="//form/group[2]/field/tree/field[@name='quantity']" position="after">
	        		<field name="note"/>
	        	</xpath>
	        </field>
	    </record>
	</data>
</odoo>