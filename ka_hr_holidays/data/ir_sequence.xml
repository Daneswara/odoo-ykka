<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="employee_dinas_sequence_dir" model="ir.sequence">
			<field name="name">Employee Surat Tugas</field>
			<field name="code">employee.surat.tugas.dir</field>
			<field name="padding">3</field>
			<field name="suffix">/DIR/SPJ/%(range_year)s</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.kantor_direksi"/>
		</record>

		<record id="employee_dinas_sequence_kba" model="ir.sequence">
			<field name="name">Employee Surat Tugas</field>
			<field name="code">employee.surat.tugas.kba</field>
			<field name="padding">3</field>
			<field name="suffix">/KBA/SPJ/%(range_year)s</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.pg_kebonagung"/>
		</record>

		<record id="employee_dinas_sequence_tk" model="ir.sequence">
			<field name="name">Employee Surat Tugas</field>
			<field name="code">employee.surat.tugas.tk</field>
			<field name="padding">3</field>
			<field name="suffix">/TK/SPJ/%(range_year)s</field>
			<field name="use_date_range">True</field>
			<field name="number_increment">1</field>
			<field name="company_id" ref="base.pg_trangkil"/>
		</record>
	</data>
</odoo>